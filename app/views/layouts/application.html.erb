<!DOCTYPE html>
<html>
  <head>
    <title>BarkPups</title>
    <%= favicon_link_tag asset_path('BarkPupsLogoBG.png') %>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag "application", "data-turbo-track": "reload" %>
    <%= javascript_importmap_tags %>
  </head>

  <body data-controller="window">
    <div class="fs-image-container" data-window-target="imageContainer">
      <img src="" alt="full-screen image" data-window-target="image">
    </div>
    <%= render 'shared/navbar' %>
    <%= yield %>
  </body>
</html>
